<h1>Add a Review Here</h1>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit "add this review to #{@restaurant.name}", class: "btn btn-primary" %>
<% end %>

<%= link_to "Back to list", restaurants_path, class: "btn btn-outline-dark"%>

